              "rssFrequency":"",
              "disabled":"0",
              "extraData":"",
              "foreignKey":"",
              "optedin":"0",
              "bouncecount":"0",
              "uniqid":"unique_id",
              "htmlemail":"1",
              "subscribepage":"",
              "rssfrequency":"",
